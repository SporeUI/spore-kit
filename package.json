{
  "name": "spore-kit",
  "version": "0.2.0",
  "description": "原子化工具函数库",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-package": "jest num",
    "cover": "jest --coverage",
    "cover-package": "gulp clean-cover && jest --coverage num",
    "publish-packages": "lerna publish",
    "vuepress-dev": "vuepress dev docs",
    "vuepress-build": "vuepress build docs",
    "doc-build": "sh ./task/doc-build.sh ./packages",
    "doc": "npm run cover && npm run doc-build && npm run vuepress-build",
    "prepublishOnly": "npm test && gulp"
  },
  "lint-staged": {
    "packages/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "dist",
    "packages",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SporeUI/spore-kit.git"
  },
  "author": "pillar0514@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SporeUI/spore-kit/issues"
  },
  "homepage": "https://github.com/SporeUI/spore-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "babel-eslint": "^10.0.3",
    "babel-preset-power-assert": "^3.0.0",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "documentation": "^12.1.4",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-for-of-loops": "^1.0.1",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-browserify": "^0.5.1",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.2.5",
    "intelli-espower-loader": "^1.0.1",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "lerna": "^3.19.0",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.0",
    "power-assert": "^1.6.1",
    "puppeteer": "^5.2.1",
    "vuepress": "^1.5.2"
  },
  "dependencies": {}
