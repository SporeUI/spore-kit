{
  "name": "spore-kit",
  "version": "0.0.0-development",
  "description": "原子化工具函数库",
  "main": "index.js",
  "scripts": {
    "build": "gulp",
    "test": "jest  --coverage ./test/unit",
    "test-package": "jest ./test/unit/num.test.js",
    "cover-package": "gulp clean-cover && jest --coverage ./test/unit/num.test.js",
    "vuepress-dev": "vuepress dev docs",
    "vuepress-build": "vuepress build docs",
    "doc-build": "node ./scripts/docs.js && sh ./task/doc-build.sh ./packages",
    "doc": "npm run cover && npm run doc-build && npm run vuepress-build",
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint ./packages",
    "cz": "git add . && git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SporeUI/spore-kit.git"
  },
  "bugs": {
    "url": "https://github.com/SporeUI/spore-kit/issues"
  },
  "homepage": "https://github.com/SporeUI/spore-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^5.1.0",
    "documentation": "^12.1.4",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-no-for-of-loops": "^1.0.1",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-browserify": "^0.5.1",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.2.5",
    "intelli-espower-loader": "^1.0.1",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-useragent-mock": "^0.1.1",
    "jquery": "^3.6.0",
    "jsdom": "^18.0.0",
    "lerna": "^3.19.0",
    "lint-staged": "^10.2.11",
    "semantic-release": "^18.0.0",
    "vuepress": "^1.5.2"
  },
  "dependencies": {
    "document-offset": "^1.0.4",
    "js-cookie": "^3.0.1",
    "klass": "^1.4.1",
    "lodash": "^4.17.21",
    "url-parse": "^1.5.3"
  },
  "lint-staged": {
    "packages/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./hooks/pre-commit.sh && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "packages",
    "index.js"
  ],
  "author": "pillar0514@gmail.com",
  "license": "MIT"
}
